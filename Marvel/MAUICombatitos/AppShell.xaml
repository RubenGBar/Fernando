<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUICombatitos.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUICombatitos.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MAUICombatitos">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent
            Title="Ranking"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="Puntuar Combate"
            ContentTemplate="{DataTemplate local:PuntuarCombate}"
            Route="PuntuarCombate" />
    </FlyoutItem>

</Shell>
